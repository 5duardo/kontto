{
  "expo": {
    "name": "Kontto",
    "slug": "kontto",
    "version": "1.0.0",
    "icon": "./assets/kontto.png",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0D1117"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kontto.app",
      "icon": "./assets/kontto.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/kontto.png",
      "adaptiveIcon": {
        "backgroundColor": "#0D1117"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kontto.app",
      "permissions": []
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "988edac4-2aae-4db0-9ccb-e8e1a07f0992"
      }
    },
    "owner": "5duardo",
    "plugins": [
      "expo-system-ui",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Neo Sans Std Regular.otf"
          ]
        }
      ]
    ]
  }
}